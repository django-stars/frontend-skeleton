{
  "roots": ["./src/app"],
  "testRegex": "/src/app/.*\\.test.(js|jsx)$",
  "unmockedModulePathPatterns": [
    "./node_modules/react"
  ],
  "moduleNameMapper": {
    "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
     ".*?\\.(css|scss)$": "<rootDir>/mocks/styleMock.js"
  },
  "setupFiles": [
    "./test-setup.js"
  ],
  "modulePaths": [
    "src/app"
  ]
}
