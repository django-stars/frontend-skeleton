  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/django-stars/frontend-skeleton.git"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-plugin-ng-annotate": "~0.3.2",
    "babel-plugin-syntax-decorators": "~6.8.0",
    "babel-polyfill": "~6.9.1",
    "babelify": "^7.3.0",
    "browserify": "~13.0.1",
    "browserify-shim": "^3.8.10",
    "compass-importer": "^0.2.4",
    "connect-inject": "~0.4.0",
    "del": "^2.2.0",
    "express": "^4.13.1",
    "gulp": "^3.9.0",
    "gulp-angular-protractor": "~0.1.1",
    "gulp-angular-templatecache": "^1.7.0",
    "gulp-changed": "^1.2.1",
    "gulp-clean-css": "^2.0.8",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-livereload": "^3.8.0",
    "gulp-ng-annotate": "~2.0.0",
    "gulp-pug": "~3.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-template": "~4.0.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "http-proxy-middleware": "~0.15.2",
    "jasmine-core": "2.4.1",
    "karma": "^0.13.9",
    "karma-browserify": "~5.0.5",
    "karma-jasmine": "~1.0.2",
    "karma-phantomjs-launcher": "~1.0.0",
    "live-reload": "^1.1.0",
    "livereload-js": "^2.2.2",
    "morgan": "^1.6.1",
    "phantomjs-prebuilt": "~2.1.7",
    "run-sequence": "^1.1.1",
    "sprity": "1.0.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "angular": "~1.5.5",
    "angular-animate": "^1.5.3",
    "angular-sanitize": "^1.4.7",
    "angular-ui-router": "~0.3.0",
    "ionic-native": "^1.3.10",
    "ionic-npm": "^1.0.2",
    "jquery": "~2.2.4",
    "lodash": "~4.13.1"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angular-mocks": "./node_modules/angular-mocks/angular-mocks.js",
    "ionic": "ionic-npm/js/ionic.js",
    "ionic-angular": "ionic-npm/js/ionic-angular.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "depends": "jquery:jQuery",
      "exports": "angular"
    },
    "angular-mocks": {
      "depends": "angular",
      "exports": "angular.mock"
    },
    "ionic": {
      "exports": "ionic"
    },
    "ionic-angular": {
      "depends": "angular",
      "exports": "angular.module('ionic')"
    }
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "android",
    "ios"
  ]
}
