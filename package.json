    "autoprefixer": "^6.7.2",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-syntax-decorators": "6.13.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
