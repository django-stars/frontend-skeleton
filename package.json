    "electron-packager": "^8.5.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.0",
