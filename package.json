  "dependencies": {
    "lodash": "~4.13.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.2",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.8.1",
    "redux-observable": "^0.13.0",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.2.0"
  },
  "scripts": {
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --inline",
    "build": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js",
    "electron": "NODE_ENV=development node ./node_modules/.bin/electron .",
    "package-mac": "yarn build && ./node_modules/.bin/electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/static/img/icon-app.icns --prune=true --out=build",
    "package-win": "yarn build && ./node_modules/.bin/electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=dist/static/img/icon-app.ico --prune=true --out=build --version-string.CompanyName=CE --version-string.FileDescription=CE",
    "package-linux": "yarn build && ./node_modules/.bin/electron-packager . --overwrite --platform=linux --arch=x64 --icon=dist/static/img/icon-app.png --prune=true --out=build"
  }
}
